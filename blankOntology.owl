@prefix : <http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/ContactTracingOntology> .

<http://www.semanticweb.org/ContactTracingOntology> rdf:type owl:Ontology .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#dateTime
xsd:dateTime rdfs:comment "So, the date property has been eliminated, and most people suggest when using datetime to just get at a date use midnight in universal time. Ex: 2001-10-12T00:00:00Z" .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#allowAccess
:allowAccess rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Organization
                                         :Place
                                       )
                         ] ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Data
                                        :Visit
                                        owl:Thing
                                      )
                        ] ;
             rdfs:comment "The records that the Organization is willing to allow automatic access to" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#assigned
:assigned rdf:type owl:ObjectProperty ;
          rdfs:domain :User ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Organization
                                     :Person
                                     :Place
                                     :Visit
                                   )
                     ] ,
                     [ rdf:type owl:Class ;
                       owl:unionOf ( :Organization
                                     :Place
                                     :Visit
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :isCaseFor ;
                                       owl:someValuesFrom :Disease
                                     ]
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :isContactFor ;
                                       owl:someValuesFrom :Disease
                                     ]
                                   )
                     ] .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#belongsTo
:belongsTo rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Organization
                                       :Place
                                       :Transportation
                                     )
                       ] ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Organization
                                      :Person
                                      :Place
                                    )
                      ] .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#canAccess
:canAccess rdf:type owl:ObjectProperty ;
           rdfs:domain :User ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Data
                                      :Visit
                                      owl:Thing
                                    )
                      ] ;
           rdfs:comment "So, the owner of organization data can allow users with specific roles access to their records (provided they have a good reason - this isn't willy-nilly access records, but for accessing records directly relevant to contact tracing a specific case)." .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#causes
:causes rdf:type owl:ObjectProperty ;
        rdfs:domain :Disease ;
        rdfs:range :Symptom .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#coordinator
:coordinator rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :visitProperty ;
             rdfs:domain :Visit ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Organization
                                        :Person
                                        :Place
                                      )
                        ] ;
             rdfs:comment "Not original platys property" ,
                          """Whoever organized a visit, and the entity you should contact first to get information about contacts.

Especially useful in cases like conferences, where the organizer may not own the space or be physically present at the event.""" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#dataType
:dataType rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Data
                                      :Data_Type
                                    )
                      ] ;
          rdfs:range :Data_Type .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#diseaseExposure
:diseaseExposure rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :visitProperty ;
                 rdfs:domain :Visit ;
                 rdfs:range :Disease .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#endAt
:endAt rdf:type owl:ObjectProperty ;
       rdfs:domain :Transportation ;
       rdfs:range :Location .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasAddress
:hasAddress rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasLocation ;
            rdfs:domain :Place ;
            rdfs:range :Address ;
            rdfs:comment "Not original Platys property -- maps a Place to an address" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasAmbiance
:hasAmbiance rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :placeProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Place
                                         :Place_Type
                                         :Transportation
                                         :Transportation_Type
                                       )
                         ] ;
             rdfs:range :Ambiance .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasAvailability
:hasAvailability rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :personalInformation ;
                 rdfs:range :Availability .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasComorbidity
:hasComorbidity rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :personalInformation ;
                rdfs:domain :Person ;
                rdfs:range :Comorbidity .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasContactInformation
:hasContactInformation rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :personalInformation ,
                                          owl:topObjectProperty ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( :Organization
                                                   :Person
                                                   :Place
                                                   :User
                                                 )
                                   ] ;
                       rdfs:range :Contact_Information .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasDemographic
:hasDemographic rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :personalInformation ;
                rdfs:domain :Person ;
                rdfs:range :Demographic .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasDomain
:hasDomain rdf:type owl:ObjectProperty ;
           rdfs:domain :Regulation ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Organization
                                      :Place
                                      :User
                                    )
                      ] ;
           rdfs:comment "Regulations only apply over a specific region, and they apply to a specific thing. In order for a regulation to be applied, it needs to be applied to an overlap of the right place in the right location." .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasEmail
:hasEmail rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasContactInformation ;
          rdfs:range :Email .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasEmergencyContact
:hasEmergencyContact rdf:type owl:ObjectProperty ;
                     rdfs:domain :Person ;
                     rdfs:range :Person .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasEmployer
:hasEmployer rdf:type owl:ObjectProperty ;
             rdfs:domain :Job ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Organization
                                        :Person
                                        :Place
                                      )
                        ] ;
             rdfs:comment "Remember, Job is specific, and should be specific to each individual job." .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasEthnicity
:hasEthnicity rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasDemographic ;
              rdfs:range :Ethnicity .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasGender
:hasGender rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasDemographic ;
           rdfs:range :Gender .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasHouseholdMember
:hasHouseholdMember rdf:type owl:ObjectProperty ;
                    rdfs:domain :Person ;
                    rdfs:range :Person .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasJob
:hasJob rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :personalInformation ;
        rdfs:domain :Person ;
        rdfs:range :Job .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :placeProperty ;
             owl:inverseOf :mapsTo ;
             rdfs:domain :Place ;
             rdfs:range :Location ;
             rdfs:comment "They have this one with Range \"Place,\" which doesn't really help our application, so I'm including an alteration." .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasPhoneNumber
:hasPhoneNumber rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasContactInformation ;
                rdfs:range :Phone_Number .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasProxy
:hasProxy rdf:type owl:ObjectProperty ;
          rdfs:domain :Person ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Organization
                                     :Person
                                     :Place
                                   )
                     ] ;
          rdfs:comment "Who represents this person? (Ex: A minor represented by a parent. You can also be represented by yourself.)" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasRace
:hasRace rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasDemographic ;
         rdfs:range :Race .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasRole
:hasRole rdf:type owl:ObjectProperty ;
         rdfs:domain :User ;
         rdfs:range :Role .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasSymptom
:hasSymptom rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :personalInformation ;
            rdfs:domain :Person ;
            rdfs:range :Symptom .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#hasTribalAffiliation
:hasTribalAffiliation rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :hasDemographic ;
                      rdfs:range :Tribal_Affiliation .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#in
:in rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf :placeProperty ;
    rdf:type owl:TransitiveProperty ;
    rdfs:domain :Location ;
    rdfs:range :Location ;
    rdfs:comment "To map to city/state/country to the right spot. Can also map to a position" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#isCaseFor
:isCaseFor rdf:type owl:ObjectProperty ;
           rdfs:domain :Person ;
           rdfs:range :Disease .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#isContactFor
:isContactFor rdf:type owl:ObjectProperty ;
              rdfs:domain :Person ;
              rdfs:range :Disease .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#isProbableCaseFor
:isProbableCaseFor rdf:type owl:ObjectProperty ;
                   rdfs:domain :Person ;
                   rdfs:range :Disease ;
                   rdfs:comment "For contacts who have developed symptoms or for some other reason are suspected of being active cases but have not been tested" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#jobType
:jobType rdf:type owl:ObjectProperty ,
                  owl:TransitiveProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Job
                                     :Job_Type
                                   )
                     ] ;
         rdfs:range :Job_Type ;
         rdfs:comment "Can have more than one" ,
                      "Can nest job types (ex. Customer Service to Cashier)" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#livesAt
:livesAt rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :placeProperty ;
         rdfs:domain :Person ;
         rdfs:range :Place ;
         rdfs:comment "Not originally Platys property" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#mapsTo
:mapsTo rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :placeProperty ;
        rdfs:domain :Location ;
        rdfs:range :Place .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#medicationType
:medicationType rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Medication
                                            :Medication_Type
                                          )
                            ] ;
                rdfs:range :Medication_Type .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#organizationType
:organizationType rdf:type owl:ObjectProperty ,
                           owl:TransitiveProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Organization
                                              :Organization_Type
                                            )
                              ] ;
                  rdfs:range :Organization_Type .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#outbreakInvestigation
:outbreakInvestigation rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :visitProperty ;
                       rdfs:domain :Visit ;
                       rdfs:range :Disease ;
                       rdfs:comment "Indicates the Visit has been flagged as the source of a potential outbreak of some disease." .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#owns
:owns rdf:type owl:ObjectProperty ;
      rdfs:domain [ rdf:type owl:Class ;
                    owl:unionOf ( :Organization
                                  :Place
                                )
                  ] ;
      rdfs:range :Data .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#partOf
:partOf rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :placeProperty ;
        rdfs:domain :Place ;
        rdfs:range :Place .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#personalInformation
:personalInformation rdf:type owl:ObjectProperty ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( :Person
                                                 :User
                                               )
                                 ] ;
                     rdfs:range :Personal_Information .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#placeProperty
:placeProperty rdf:type owl:ObjectProperty ;
               rdfs:comment "These maybe shouldn't all be subproperties of this property, but since these are all Platys related they feel like they should be grouped." .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#placeType
:placeType rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :placeProperty ;
           rdf:type owl:TransitiveProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Place
                                       :Place_Type
                                     )
                       ] ;
           rdfs:range :Place_Type ;
           rdfs:comment "Dislike the name mapsTo., but feel like the lexicology should be consistent, hence the equivalent property." .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#primaryAssigned
:primaryAssigned rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :assigned ;
                 rdf:type owl:InverseFunctionalProperty ;
                 rdfs:domain :User ;
                 rdfs:range :Person ;
                 rdfs:comment "For contacts elucidated from a Visit being investigated by more than one User - once the contact is discovered, they should be assigned to a specific User." ,
                              "Since a contact may be updated to a case and it makes sense to use the same contact tracer that was primarily assigned to investigate the contacts of that new case, there is no rule specifying that the Person has to be a contact for some disease since they may be upgraded to case." .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#primaryEmail
:primaryEmail rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasEmail ;
              rdf:type owl:FunctionalProperty .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#primaryLanguage
:primaryLanguage rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :speaksLanguage ;
                 rdf:type owl:FunctionalProperty .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#primaryPhoneNumber
:primaryPhoneNumber rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasPhoneNumber ;
                    rdf:type owl:FunctionalProperty .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#recordIn
:recordIn rdf:type owl:ObjectProperty ;
          rdfs:domain :Person ;
          rdfs:range :Data .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#represents
:represents rdf:type owl:ObjectProperty ;
            rdfs:domain :User ;
            rdfs:range :Organization .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#speaksLanguage
:speaksLanguage rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :personalInformation ;
                rdfs:range :Language .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#startAt
:startAt rdf:type owl:ObjectProperty ;
         rdfs:domain :Transportation ;
         rdfs:range :Location .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#subData
:subData rdf:type owl:ObjectProperty ,
                  owl:TransitiveProperty ;
         rdfs:domain :Data ;
         rdfs:range :Data .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#subVisit
:subVisit rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :visitProperty ;
          rdf:type owl:TransitiveProperty ;
          rdfs:domain :Visit ;
          rdfs:range :Visit ;
          rdfs:comment "For when you visit a place-within-a-place, most likely used in cases where visiting the larger place doesn't necessarily create close contacts (ex: School vs. a specific classroom)" ,
                       "Not original Platys property" ,
                       "While technically all people who visited the subvisit should have visited the supervisit, so long as the Person in contained in the visit chain (assuming they will eventually be a contact) they probably do not need to be directly linked with the supervisit" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#takesMedication
:takesMedication rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :personalInformation ;
                 rdfs:domain :Person ;
                 rdfs:range :Medication .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#transportationType
:transportationType rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :placeProperty ;
                    rdf:type owl:TransitiveProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :Transportation
                                                :Transportation_Type
                                              )
                                ] ;
                    rdfs:range :Transportation_Type .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#userDisease
:userDisease rdf:type owl:ObjectProperty ;
             rdfs:domain :User ;
             rdfs:range :Disease .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#userDomain
:userDomain rdf:type owl:ObjectProperty ;
            rdfs:domain :User ;
            rdfs:range :Location ;
            rdfs:comment "Most important for Contact Tracers specifically" ,
                         "Where the User functions/what locations the user should cover" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#viewedBy
:viewedBy rdf:type owl:ObjectProperty ;
          rdfs:domain :Data ;
          rdfs:range :User ;
          rdfs:comment "To keep track of who has seen what (which businesses may want to solicit and ask about)" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#visitData
:visitData rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :visitProperty ;
           rdfs:domain :Visit ;
           rdfs:range :Data .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#visitProperty
:visitProperty rdf:type owl:ObjectProperty ;
               rdfs:domain :Visit .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#visitRoster
:visitRoster rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :visitProperty ;
             rdfs:domain :Regular_Visit ;
             rdfs:range :Data ;
             rdfs:comment "For Regular Visits on a schedule" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#visitedBy
:visitedBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :visitProperty ;
           rdfs:domain :Visit ;
           rdfs:range :Person .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#visitedPlace
:visitedPlace rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :visitProperty ;
              rdfs:domain :Visit ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :Place
                                         :Transportation
                                       )
                         ] ;
              rdfs:comment "Not original platys property" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#waiveProtection
:waiveProtection rdf:type owl:ObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Organization
                                             :Person
                                             :Place
                                             :User
                                           )
                             ] ;
                 rdfs:range :Regulation ;
                 rdfs:comment "Whoever is being protected can waive the protections. Sometimes. (Like I don't think students can waive FERPA). Will need rules to see if the entity waiving the protection has the authority to waive the protection." .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdfs:comment "I'm paranoid that I'm missing something" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#canIsolate
:canIsolate rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :isCompliant .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#canQuarantine
:canQuarantine rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :isCompliant .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#canSeparateFromHousehold
:canSeparateFromHousehold rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf :isCompliant .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#complete
:complete rdf:type owl:DatatypeProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Person
                                      :Visit
                                    )
                      ] ;
          rdfs:range xsd:boolean ;
          rdfs:comment "Can also indicate when a Visit has all contacts unearthed on it. Will likely not be automatically changed, but instead manually changed." ,
                       """Indicates whether a Person instance has:
a) A Name
b) Contact Information of some kind, ideally a phone number

It does not indicate whether all of the information about Person has be unconvered.""" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#dateOfBirth
:dateOfBirth rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :dateProperty .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#dateOfClear
:dateOfClear rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :dateProperty ;
             rdfs:comment "When you can get out of quarantine/isolation (assuming no disease develops/worsening of symptom)" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#dateOfContact
:dateOfContact rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :dateProperty ;
               rdfs:comment "When the contact tracer gets in contact with cases/contacts" .


###  http://www.semanticweb.org/pnora/ontologies/2021/6/7-2-fresh#dateOfContagiousness
:dateOfContagiousness rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf :dateProperty ;
                      rdfs:comment "Depends on the disease, but should be 2 days before symptoms first appear for COVID-19. If asymptomatic, should be 2 days before positive test result for COVID-19, though if that's revised to presymptomatic this date can be updated." ,
